;
; IsxDigit- Checks al to see if it is a hex digit.
;
IsxDigit	macro
		ifndef	sl_IsxDigit
stdlib		segment	para public 'slcode'
		extrn	sl_IsxDigit:far
stdlib		ends
		endif
		slcall	sl_IsxDigit
		endm
;
;
;
ToLower		macro
		local	NoCnvrt
		cmp	al, 'A'
		jb	NoCnvrt
		cmp	al, 'Z'
		ja	NoCnvrt
		or	al, 20h
NoCnvrt:
		endm
;
;
;
ToUpper		macro
		local	NoCnvrt
		cmp	al, 'a'
		jb	NoCnvrt
		cmp	al, 'z'
		ja	NoCnvrt
		and	al, 05fh
NoCnvrt:
		endm
;
;
;
IsDigit		macro
		local	NotDigit, IsDigit
		cmp	al, '0'
		jb	NotDigit 		;Note: this also clears ZF.
		cmp	al, '9'
		ja	NotDigit		;Also clears ZF.
		cmp	al, al			;Set Zero flag
NotDigit:
		endm
;
;
;
IsAlpha		macro
		local	IsAl, NotAlpha
		cmp	al, 'A'
		jb	NotAlpha		;Also clears ZF.
		cmp	al, 'Z'
		jbe	IsAl
		cmp	al, 'a'
		jb	NotAlpha
		cmp	al, 'z'
		ja	NotAlpha
IsAl:		cmp	al, al			;Set ZF.
NotAlpha:
		endm
;
;
IsLower		macro
		local	NotLower
		cmp	al, 'a'
		jb	NotLower
		cmp	al, 'z'
		ja	NotLower
		cmp	al, al			;Set ZF.
NotLower:
		endm
;
;
; IsUpper-	Checks to see if the character in al is upper case.
;
IsUpper		macro
		local	NotUpper
		cmp	al, 'A'
		jb	NotUpper
		cmp	al, 'Z'
		ja	NotUpper
		cmp	al, al			;Set ZF.
NotUpper:
		endm
;
