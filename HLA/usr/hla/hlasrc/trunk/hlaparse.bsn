				int		  FieldCnt;
				int		  LargestField;

				_here;
				
				/*
				** Restore currentNS before we enter this record
				** type into the symbol table.
				*/
				
				currentNS = $<ru.saveCurNS>3;
				RecNS = $<ru.saveRecNS>3;
				RecGlobal = $<ru.saveRecGlobal>3;

