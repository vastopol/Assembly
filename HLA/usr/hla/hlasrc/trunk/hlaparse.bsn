						_case( tInt64 )
						_case( tInt128 )
						_case( tReal32 )
						_case( tReal64 )
						_case( tReal80 )
  			    		_case( tCset )

							_here;
							memcpy( &$<v>$, &$<v>1, sizeofSymNode );
							subval
							( 
								&$<v.u.lwordval>$[0], 
								&$<v.u.lwordval>3[0], 
