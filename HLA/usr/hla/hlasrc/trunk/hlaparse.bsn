		'['					/* $1 */
		SetElementCnt		/* $2 */
		amGutz				/* $3 */
		zeroOrMoreGutz		/* $4 */
		{
			int				index;
			SymNode_t 		*s;
			
			_here;
			mergeMem( YYM &$<mem>3, YYM &$<mem>4 );
			
			// Check for the use of ESP as an index register:
			
			_if( $<mem.index>3 == reg_esp )
			
				_if( $<mem.base>3 == reg_esp || $<mem.scale>3 > 1 )
				
					yyerror( "ESP cannot be used as an index register" );
					
