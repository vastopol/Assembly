//	Allows us to have concatenated bracket expressions, e.g.,
//		[eax][ebx*2][4]

zeroOrMoreGutz:
		'['				/* $1 */
		amGutz			/* $2 */
		zeroOrMoreGutz	/* $3 */
		{
			_here;
			$<mem>$ = $<mem>2;
			mergeMem( YYM &$<mem>$, YYM &$<mem>3 );
		}
		
	|	'['				/* $1 */
		Displacement2	/* $2 */
		Rbrack			/* $3 */
		zeroOrMoreGutz	/* $4 */
		{
			_here;
			$<mem>$ = $<mem>4;
			$<mem.disp>$ += $<v.u.unsval>2;
		}
	
	|	/* empty */
		{
			_here;
			$<mem.base>$ = -1;
			$<mem.index>$ = -1;
			$<mem.scale>$ = 0;
			$<mem.disp>$ = 0;
		}
	;



