					SetConst
					( 
						YYS &$<v>$, 
						$<v.pType>1, 
						$<v.Type>1,
						YYS &v				/* Dummy value */
					);

					_switch( $<v.pType>1 )

						/*
						** For the numeric types, do subtraction.
						*/

						_case( tUns8 )
						_case( tUns16 )
						_case( tUns32 )
						_case( tUns64 )
						_case( tUns128 )
						_case( tByte )
						_case( tWord )
						_case( tDWord )
						_case( tQWord )
						_case( tTByte )
						_case( tLWord )
						_case( tInt8 )
