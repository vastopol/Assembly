					yyerror
					( 
						"Type mismatch of array operands or "
						"element value out of range" 
					);
					FreeValue( YYS &$<v>5 );


				_endif

			/*
			** If it's not an array, check to see if it is
			** a record.
			*/

			_elseif( $<s>3->pType == tRecord && $<v.pType>5 == tRecord )

				_if
				( 
					RecordsAreCompatible
					( 
						$<s>3, 
						YYS &$<v>5 
