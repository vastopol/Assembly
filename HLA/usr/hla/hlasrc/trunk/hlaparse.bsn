		Empty
		{
			_here;
			memcpy( &$<adrs>$, &$<adrs>1, sizeofYYSTYPE );
			_here;
		}
	;


Memory:
		SimpleMemoryVar		/* $1 */ 
		FieldStuff			/* $2 */
		IndexAdrsMode		/* $3 */
		{					/* $4 */

			char address[ 256 ];
			char name[ 256 ];
