EmitFalseLabel:
		{
			char label[ 256 ];

			_here;
			$<labels.l3.exitTarget>$ = LblCntr++;

			sprintf( label, "endif" sympost "%d", $<labels.l3.exitTarget>$ );
			EmitJmpLabel( label );
			EmitStmtLblNum( "false" sympost "%d", $<labels.l2.falseTarget>0 );
			_here;
		}
	;








PutElseIfSTL:
		{
			_here;
			EmitStmtLblNum( "false" sympost "%d", $<labels.l2.falseTarget>-2 );
