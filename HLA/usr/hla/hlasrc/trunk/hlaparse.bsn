			$<s>7->u.proc.parms->u.proc.parms = s;
			$<s>7->u.proc.parms->u.proc.returns = $<v.u.proc.returns>1;
			$<s>7->u.proc.parms->u.proc.use = $<v.u.proc.use>1;
