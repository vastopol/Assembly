// Moved here from hlautils.

static char*
getFileStr( int startPosn )
_begin( getFileStr )

	char	*t;
	int		CurPosn;
	int		size;
	
	_here;
	CurPosn = asmBuf->offset;
	size = CurPosn - startPosn + 1;
	t = malloc2( size+2 );
	strncpy( t, asmBuf->base + startPosn, size );
	t[ size ] = '\0';
	asmBuf->offset = startPosn;
	_return t;
				
_end( getFileStr )






																	 




