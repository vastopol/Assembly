 /*
 ** doScanBlock-
 **
 **	A recursive entry to the parse that handles the #scan sequence:
 **
 **		#scan( string, StrArrayID )
 **
 **	This production converts each token in the 1st string argument
 ** to a lexeme string and stores each of these lexemes in the
 ** VAL string array specified by StrArrayID.
 */

makeValID:
		lclUndefID
		{
			union YYSTYPE v;
			
			// Create a dummy entry that we can change:
			
			ClrConst
			(
				YYS &v,
				tUns32,
				&uns32_ste
			);
			
			InsertSym
			(
				$<idStr>1,
				&uns32_ste,
				tUns32,
				cValue,
				0,						/* Arity 			*/
				NULL,					/* Dimensions		*/
				0,						/* NumElements		*/
				&v,						/* The Value		*/
				4,						/* Object Size		*/
				0,						/* Current Offset	*/
				NULL,					/* Static Name		*/
				NULL,					/* Base				*/
				NULL,					/* Fields			*/
				0						/* FieldCnt			*/
			);
			$<s>$ = SymbolTable;
		
		}

	|	DefinedID
		{
			$<s>$ = $<s>1;
		
		}
	;
	
doScanBlock:
		scanblockTkn	/* $1 */
		Lparen			/* $2 */
		ConstOnlyExpr	/* $3 */
		comma			/* $4 */
		makeValID		/* $5 */
		comma			/* $6 */
		makeValID		/* $7 */
		Rparen			/* $8 */
		{				/* $9 */

			_here;
			doScanBlock(  &yylval, YYS &$<v>3, $<s>5, $<s>7 ); 
			YYACCEPT
		}
	;



