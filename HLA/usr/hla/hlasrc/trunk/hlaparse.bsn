					0,					/* Offset				*/
					NULL,				/* Static Name			*/
					NULL,				/* Record BASE address	*/
					NULL,				/* Fields				*/
					0,					/* FieldCnt				*/ 
					NULL,				/* CurField				*/
					0					/* CurIndex				*/ 
