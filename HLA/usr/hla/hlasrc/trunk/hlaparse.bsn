 /* Handle (constant) identifiers here. */
 /*                                     */
 /***************************************/





ConstExpr:
		DefinedConstID
		{
			_here;

			/*
			** Okay, we've got an identifier that is either
			** a cConstant or cValue object.  Pass along the
			** appropriate information to the calling code.
			*/

			_if
			( 
					$<s>1->pType == tString 
				||	$<s>1->pType == tZString 
				||	$<s>1->pType == tText 
			)
			
				SetConst
				( 
					YYS &$<v>$, 
					tString, 
					&string_ste, 
					YYS &true_ste
