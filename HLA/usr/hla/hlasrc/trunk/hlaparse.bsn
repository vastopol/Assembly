				TypeName = "real80";


			// Note: for cls objects, sym==type if you have a type dcl.

			_elseif( $<cls.type>3 != NULL && $<cls.sym>3 != $<cls.type>3 )
 
				_if( $<cls.cls>3 & cfy_IsPointer )
				
					_if( $<cls.sym>3->Base != NULL )
					
						
						strcpy( ptrName, "pointer to " );
						strncpy
						( 
							&ptrName[11],
							$<cls.sym>3->Base->TrueName,
							244
						);
						ptrName[255] = '\0';  
						TypeName = ptrName;
											
					_else
					
