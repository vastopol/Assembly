void
SetSeg( char* src, char* dest, char* defaultStr )
_begin( SetSeg )

	_here;
	_if( strlen( src ) == 0 || *src == '.' )

		strcpy( dest, defaultStr );

	_elseif( stricmp( src, "_text" ) == 0 )

		strcpy( dest, ".text" );

	_elseif( stricmp(src, "_data" ) == 0 )
	
		strcpy( dest, ".data" );
		
	_elseif( stricmp( src, "_bss" ) == 0 )
	
		strcpy( dest, ".bss" );
		
	_elseif( stricmp( src, "CONST" ) == 0 )
	
		strcpy( dest, ".text" );
		
	_else
	
		strcpy( dest, src );
		
	_endif 

_end( SetSeg )



