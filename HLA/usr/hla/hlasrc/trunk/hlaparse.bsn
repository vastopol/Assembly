		LocalProcID				/* $1  */ 
		SaveOffset 				/* $2  */
		SaveDirection 			/* $3  */
		SaveContext 			/* $4  */
		colonProcedure2			/* $5  */
		MatchParms				/* $6  */
		PatchLocals				/* $7  */
		newProcOptionsSemicolon	/* $8  */
		externalTkn				/* $9  */
		{						/* $10 */
			_here;
			$<idStr>$ = hlastrdup2( "" );
		}
		OptionalExtName		/* $11 */
		Semicolon			/* $12 */
		{
			char msg[ 256 ];
			
			sprintf
