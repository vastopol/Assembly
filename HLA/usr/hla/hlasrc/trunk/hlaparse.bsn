		}
		doOptionalParms
		{ 
			_here;
			currentNS = $<ru.saveCurNS>1;
			//ProcGlobal = $<ru.saveProcGlobal>1; // see above
			ProcNS = $<ru.saveProcNS>1;
			$<v.u.intval>$ = $<v.u.intval>3;
			parmLookup = 0;
			fastLookup = 0;
			(void) $<s>2;	//shut up bison 
