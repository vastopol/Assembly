				// union constants:
				
				SymbolTable->CurField = $<v.CurField>5;
				SymbolTable->CurIndex = $<v.CurIndex>5;


			/*
			** If it's not an array, union, or record, check to see if it is
			** a real128.
			*/

			_elseif
			( 
					$<s>3->pType == tReal128 
				&&	$<v.pType>5 == tReal128
				&&	GetBaseType( $<s>3 ) == GetBaseType( $<v.Type>5 )
			)

