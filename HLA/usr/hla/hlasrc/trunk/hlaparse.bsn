ConstFunc:	modregrmTkn		// $1  
			Lparen			// $2  
			ConstExpr		// $3
			comma			// $4
			modRegRmMem		// $5 
			Rparen			// $6
			{
				unsigned		modRM;
				unsigned		hasSib;
				unsigned		sib;
				unsigned		dispType;
				int				disp;
				char			sibStr[32];
				char			dispLabel[256];
				char			dispStr[ 512];
				char			rsltStr[1024];
				union	YYSTYPE value;
				
				_here;
				_if( !checkSmallInt( YYS &$<v>3 ) || $<v.u.unsval>3 > 7 )

					yyerror( "First operand must be in the range 0..7" );
					strcpy( rsltStr, "" );

				_else
				
					EncodeHLAAdrs
					(
						&$<adrs>5,
						&modRM,
						&hasSib,
						&sib,
						&dispType,
						&disp,
						dispLabel
					);
					_if( hasSib )
					
						sprintf( sibStr, " byte $%x;\n", sib );
						
					_else
					
						strcpy( sibStr, "" );
						
					_endif
					_switch( dispType )
					
						_case( 0 )
						
							strcpy( dispStr, "" );
							
						_endcase
						
						_case( 1 )
						
							sprintf( dispStr, " byte %d;\n", disp );
							
						_endcase
						
						_case( 2 )
						
							sprintf( dispStr, " dword %d;\n", disp );
							
						_endcase
						
						_case( 3 )
						
							sprintf( dispStr, " dword %s;\n", dispLabel );
							
						_endcase
						
						_default
						
							yyerror
							( 
								"Bad EncodeAdrs dispType (internal HLA error)" 
							);
							strcpy( dispStr, "" );
							
					_endswitch
					sprintf
					(
						rsltStr,
						" byte $%x;\n"		// mod-reg-r/m
						"%s"				// sib
						"%s",				// disp
						modRM,
						sibStr,
						dispStr
					);
					
				_endif
				value.v.u.strval = hlastrdup2( rsltStr );
				SetConst
				( 
					YYS &$<v>$, 
					tString, 
					&string_ste,
					YYS &value
				);				
				_here;
			}
						
		;






