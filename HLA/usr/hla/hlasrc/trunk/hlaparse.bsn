				$<mem.base>3,
				$<mem.index>3,
				$<mem.scale>3,
				$<mem.disp>3
			);
			
			// Create a dummy symbol for the <adrs.Sym> field so it is never NULL.
			
			s = SymbolTable;
			InsertSym
			( 
				" (anonsym) ", 
				&dword_ste,		/* Default is int32 object.		*/
				tDWord,
				cStatic,		/* Not really static, but...	*/ 
				0,				/* Arity						*/
				NULL,			/* Dimensions					*/
				0, 				/* NumElements					*/
				0,				/* No initialization			*/
				0,				/* Object size					*/
				0,				/* Offset						*/
				NULL,			/* Static Name					*/
				NULL,			/* Base							*/
				NULL,			/* Fields						*/
				0				/* FieldCnt						*/
			);
			$<adrs.Sym>$ = SymbolTable;
			SymbolTable = s;
			_here;
		}


