			** If it's not an array or record, check to see if it is
			** a union.
			*/

			_elseif
			( 
					$<s>3->pType == tUnion 
				&&	$<v.pType>5 == tUnion
				&&	GetBaseType( $<s>3 ) == GetBaseType( $<v.Type>5 )
			)

