CleanUpWhile( void )
_begin( CleanUpWhile )

    _here;
    _if( WhileSP >= 0 )

        deleteSourceBuf();
        free2( vss WhileStack[ WhileSP ].line );
        --WhileSP;

    _endif

    _here;

_end( CleanUpWhile )


