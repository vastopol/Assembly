                ClrConst
                (
                    YYS &yylval->v,
                    tWChar,
                    &wchar_ste
                );
                yylval->v.u.unsval = value; 
                _here;
                _return wcharconst; 
            }



 /*
 ** Handle character hexadecimal literal constants here.
 */

#$[0-9a-f]+ {
                _here;
                ClrConst
                (
                    YYS &yylval->v,
                    tChar,
