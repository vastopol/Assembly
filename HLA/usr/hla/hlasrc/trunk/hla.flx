/*****************************************************************/
/*                                                               */
/* trim-                                                         */
/*                                                               */
/* Eliminates spaces at the beginning and end of the specified   */
/* string and returns a pointer to a copy of the trimmed string. */
/*                                                               */
/*****************************************************************/

static char*
trim( char *str, int LastPosn )
_begin( trim )

    int fromBack = LastPosn;
    char *s;

    _here;
    _if( fromBack > 0 )

        _while( fromBack > 0  &&  str[ fromBack ] <= ' ' )

            --fromBack;

        _endwhile
    
        str[ fromBack + 1 ] = '\0';
        
    _endif

    _while( *str != '\0'  &&  *str <= ' ' )
    
        ++str;

    _endwhile
    _here;
    s = hlastrdup2( str );
    _return s;

_end( trim )




