 /*
 ** Character literal constants that use binary numbers.
 */

#%[0-1]([0-1_]*[0-1])?  {
                unsigned value = 0;
                char *s=yytext+2;

                _here;
                _while( *s != '\0' )

                    _if( *s != '_' )
                    
                        value = ( value << 1 ) | ( *s & 1 );

                    _endif
                    ++s;

                _endwhile
                _if( !IntRange( value, 0, 255 ))
                
                    yyerror( "ASCII codes must be in the range 0..255" );
