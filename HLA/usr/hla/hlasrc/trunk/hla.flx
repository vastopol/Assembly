// doScanBlock - Handles #scan..#endscan with an undefined ID

void doScanBlock
( 
	union	YYSTYPE		*yylval, 
	union	YYSTYPE 	*theString, 
			SymNode_t 	*lexemes, 
			SymNode_t 	*tokens 
)
_begin( doScanBlock )

	char			*scannableStr;
	int				len;
	int				newlen;
	char			*endscan = " #endscan\n";
    int 			cnt = 0;
    int 			i;
	int 			tkn;
	YYLTYPE			yylloc;
    struct ScanList	*temp;
    struct ScanList	*ScanSave;
	
	_if( theString->v.pType != tString )
	
		yyerror( "#scan first argument must be a string expression" );
		exit( 1 );
		
		
	_endif
	_if( lexemes->SymClass != cValue )
	
		yyerror( "#scan second argument must be a VAL identifier" );
		exit( 1 );
		
	_endif
	_if( tokens->SymClass != cValue )
	
		yyerror( "#scan third argument must be a VAL identifier" );
		exit( 1 );
		
	_endif


	len = strlen( theString->v.u.strval );
	newlen = len + strlen( endscan );
	scannableStr = malloc( newlen + 16 );
	strcpy( scannableStr, theString->v.u.strval );
	strcat( scannableStr, endscan );
	scannableStr[ newlen ] = '\0';
	scannableStr[ newlen+1 ] = '\0';
	
	
	// Set up the string we created as the input buffer:
	
	pushBufStack( FileName, LineCnt, NULL, 0 );
    yy_switch_to_buffer( yy_scan_buffer( scannableStr, newlen+2 ));
	
	// Process all the lexemes in the string:
	
	inScan = 1;
	_forever
	
		tkn = yylex( yylval, &yylloc );
		_breakif( tkn == endscanTkn )

        temp = malloc2( sizeof( struct ScanList ));
        temp->next  = NULL;
        temp->line  = hlastrdup2( yytext );
		temp->token	= tkn;

        _if( ScanTail == NULL )

            ScanHead = temp;
            ScanTail = temp;
            
        _else

            ScanTail->next = temp;
            ScanTail = temp;

        _endif
								
	_endfor	
	  
	// We're done with the input buffer:
	
	deleteSourceBuf();
	free2( vss scannableStr );
	  


	/*
	** Count the number of tokens
	** in the scan block.
	*/

	temp = ScanHead;
	cnt = 0;
	_while( temp != NULL )

	    temp = temp->next;
	    ++cnt;

	_endwhile
	ScanSave = ScanHead;


	
	// Create the array of lexemes:
	

	FreeValue( YYS lexemes );

	lexemes->pType = tArray;
	lexemes->SymClass = cValue;
	lexemes->Type = &string_ste;
	lexemes->Arity = 1;
	lexemes->Dimensions = malloc2( sizeof( int ));
	lexemes->Dimensions[0] = cnt;
	lexemes->NumElements = cnt;
	lexemes->u.ArrayOfValues = malloc2( cnt * sizeofSymNode );
	lexemes->ObjectSize = cnt * sizeof( char* );

	_for( i=0, i<cnt, ++i )

	    lexemes->u.ArrayOfValues[i].Name = hlastrdup2( "" );
	    lexemes->u.ArrayOfValues[i].TrueName = hlastrdup2( "" );
	    lexemes->u.ArrayOfValues[i].SymClass = cConstant;
	    lexemes->u.ArrayOfValues[i].StaticName = NULL;
	    lexemes->u.ArrayOfValues[i].Base = NULL;
	    lexemes->u.ArrayOfValues[i].Fields = NULL;
	    lexemes->u.ArrayOfValues[i].FieldCnt = 0;
	    lexemes->u.ArrayOfValues[i].pType = tString;
	    lexemes->u.ArrayOfValues[i].Type = &string_ste;
	    lexemes->u.ArrayOfValues[i].Arity = 0;
	    lexemes->u.ArrayOfValues[i].Dimensions = NULL;
	    lexemes->u.ArrayOfValues[i].NumElements = 0;

	    lexemes->u.ArrayOfValues[i].u.strval = 
	        ScanHead->line;
	        
	    ScanHead = ScanHead->next;

	_endfor

	

	// Create the array of tokens:
	
	ScanHead = ScanSave;
	FreeValue( YYS tokens );

	tokens->pType = tArray;
	tokens->SymClass = cValue;
	tokens->Type = &uns32_ste;
	tokens->Arity = 1;
	tokens->Dimensions = malloc2( sizeof( int ));
	tokens->Dimensions[0] = cnt;
	tokens->NumElements = cnt;
	tokens->u.ArrayOfValues = malloc2( cnt * sizeofSymNode );
	tokens->ObjectSize = cnt * sizeof( char* );

	_for( i=0, i<cnt, ++i )

	    tokens->u.ArrayOfValues[i].Name = hlastrdup2( "" );
	    tokens->u.ArrayOfValues[i].TrueName = hlastrdup2( "" );
	    tokens->u.ArrayOfValues[i].SymClass = cConstant;
	    tokens->u.ArrayOfValues[i].StaticName = NULL;
	    tokens->u.ArrayOfValues[i].Base = NULL;
	    tokens->u.ArrayOfValues[i].Fields = NULL;
	    tokens->u.ArrayOfValues[i].FieldCnt = 0;
	    tokens->u.ArrayOfValues[i].pType = tUns32;
	    tokens->u.ArrayOfValues[i].Type = &uns32_ste;
	    tokens->u.ArrayOfValues[i].Arity = 0;
	    tokens->u.ArrayOfValues[i].Dimensions = NULL;
	    tokens->u.ArrayOfValues[i].NumElements = 0;

	    tokens->u.ArrayOfValues[i].u.unsval = 
	        ScanHead->token;
	        
	    temp = ScanHead->next;
	    free2( vss ScanHead );
	    ScanHead = temp;

	_endfor

	

_end( doScanBlock )
 


